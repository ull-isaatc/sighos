<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.example.org/SimulationListener"
	xmlns:tns="http://www.example.org/SimulationListener"
	elementFormDefault="qualified">
	<include schemaLocation="SimulationListener.xsd" />
	<complexType name="PeriodicListener">
		<complexContent>
			<extension base="tns:SimulationListener">
				<attribute name="period" type="double"/>
			</extension>
		</complexContent>
	</complexType>
</schema>
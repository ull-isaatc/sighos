@prefix : <http://www.ull.es/iis/simulation/ontologies/disease-simulation#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.ull.es/iis/simulation/ontologies/disease-simulation> .

<http://www.ull.es/iis/simulation/ontologies/disease-simulation> rdf:type owl:Ontology ;
                                                                  owl:versionIRI <http://www.ull.es/iis/simulation/ontologies/disease-simulation/1.0> ;
                                                                  :contributor "María Luz Couce Pico (https://orcid.org/0000-0003-4861-9905)" ;
                                                                  :creator "David Prieto González (https://orcid.org/0000-0003-1411-129X)" ,
                                                                           "Evelio J. González González (https://orcid.org/0000-0002-2203-3757)" ,
                                                                           "Iván Castilla Rodríguez (http://orcid.org/0000-0003-3933-2582)" ;
                                                                  rdfs:comment "Ontology designed for collecting and inferring the parameters required to create a simulation-based Health Technology Assesment"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://ainf.aau.at/ontodebug#testCase
<http://ainf.aau.at/ontodebug#testCase> rdf:type owl:AnnotationProperty .


###  http://ontologydesignpatterns.org/opla-sd#entityPosition
<http://ontologydesignpatterns.org/opla-sd#entityPosition> rdf:type owl:AnnotationProperty .


###  http://ontologydesignpatterns.org/opla-sd#entityPositionX
<http://ontologydesignpatterns.org/opla-sd#entityPositionX> rdf:type owl:AnnotationProperty .


###  http://ontologydesignpatterns.org/opla-sd#entityPositionY
<http://ontologydesignpatterns.org/opla-sd#entityPositionY> rdf:type owl:AnnotationProperty .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#TODO
:TODO rdf:type owl:AnnotationProperty .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#contributor
:contributor rdf:type owl:AnnotationProperty .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#creator
:creator rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#belongsToGroup
:belongsToGroup rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasComponent ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :GroupableModelItem
                                            :ProportionWithinGroup
                                          )
                            ] ;
                rdfs:range :Group .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#dependsOnAttribute
:dependsOnAttribute rdf:type owl:ObjectProperty ;
                    rdfs:domain :CalculatedParameter ;
                    rdfs:range :Attribute ;
                    rdfs:comment "Defines a relationship between a Valuable instance and an Attribute, where the \"hasExpression\" of the domain instance makes reference to the range instance. Hence, the hasExpression property should include a reference to that attribute, so that the final value of this Valuable is a function of the value of this attribute."@en .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#dependsOnParameter
:dependsOnParameter rdf:type owl:ObjectProperty ;
                    rdfs:domain :CalculatedParameter ;
                    rdfs:range :Parameter ;
                    rdfs:comment "Defines a relationship between a Valuable instance and a Parameter, where the \"hasExpression\" of the domain instance makes reference to the range instance. Hence, the hasExpression property should include a reference to that parameter, so that the final value of this Valuable is a function of the value of this parameter."@en .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#excludesManifestation
:excludesManifestation rdf:type owl:ObjectProperty ;
                       rdfs:domain :Manifestation ;
                       rdfs:range :Manifestation .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#followedByStrategy
:followedByStrategy rdf:type owl:ObjectProperty ;
                    rdfs:domain :Strategy ;
                    rdfs:range :Strategy .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasAge
:hasAge rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :hasAttributeValue ;
        rdfs:domain :Population ;
        rdfs:range [ rdf:type owl:Restriction ;
                     owl:onProperty :isValueOfAttribute ;
                     owl:hasValue :Attribute_Age
                   ] .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasAttributeValue
:hasAttributeValue rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :Development
                                               :Disease
                                               :Population
                                             )
                               ] ;
                   rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              rdfs:domain :Group ;
              rdfs:range :GroupableModelItem .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasConditionExpression
:hasConditionExpression rdf:type owl:ObjectProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :Development
                                                    :Guideline
                                                    :Pathway
                                                    :Strategy
                                                  )
                                    ] ;
                        rdfs:range :ConditionExpression ;
                        rdfs:comment "It allows expressing conditions based on parameters that we will find in the model. For example, we can find that certain drugs will be administered depending on the weight or age of the patient."@en .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasCost
:hasCost rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasParameter ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Development
                                     :Disease
                                     :HealthTechnology
                                     :Intervention
                                     :Manifestation
                                     :Stage
                                     :Strategy
                                   )
                     ] ;
         rdfs:range :Cost .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasDataItemType
:hasDataItemType rdf:type owl:ObjectProperty ;
                 rdfs:domain :Parameter ;
                 rdfs:range :DataItemType .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasDevelopment
:hasDevelopment rdf:type owl:ObjectProperty ;
                rdfs:domain :Disease ;
                rdfs:range :Development .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasDiagnosisCost
:hasDiagnosisCost rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasCost ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Development
                                              :Disease
                                            )
                              ] ;
                  rdfs:range :Cost .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasDiagnosisStrategy
:hasDiagnosisStrategy rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasStrategy ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( :DiagnosisIntervention
                                                  :DiagnosisStrategy
                                                  :Disease
                                                  :ScreeningStrategy
                                                )
                                  ] ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( :DiagnosisStrategy
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasComponent ;
                                                   owl:allValuesFrom :DiagnosisStrategy
                                                 ]
                                               )
                                 ] .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasDose
:hasDose rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasParameter ;
         rdfs:domain :Guideline ;
         rdfs:range :Parameter ;
         rdfs:comment "Dosage of the indicated drug"@en .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasDuration
:hasDuration rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasParameter ;
             rdfs:domain :DiseaseProgression ;
             rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasEndAge
:hasEndAge rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasParameter ;
           rdfs:domain :Manifestation ;
           rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasEpidemiologicalParameter
:hasEpidemiologicalParameter rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :hasParameter ;
                             rdfs:domain :Disease ,
                                         :Population ;
                             rdfs:range :EpidemiologicalParameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasExpressionLanguage
:hasExpressionLanguage rdf:type owl:ObjectProperty ;
                       rdfs:domain :CalculatedParameter ;
                       rdfs:range :ExpressionLanguage .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasFollowUpCost
:hasFollowUpCost rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasCost ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Development
                                             :Disease
                                             :Manifestation
                                           )
                             ] ;
                 rdfs:range :Cost .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasFollowUpStrategy
:hasFollowUpStrategy rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasStrategy ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( :Development
                                                 :Disease
                                                 :FollowUpStrategy
                                                 :LineOfTherapy
                                                 :Manifestation
                                                 :TherapeuticIntervention
                                               )
                                 ] ;
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( :FollowUpStrategy
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasComponent ;
                                                  owl:allValuesFrom :FollowUpStrategy
                                                ]
                                              )
                                ] .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasFrequency
:hasFrequency rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasParameter ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :AcuteManifestation
                                          :Guideline
                                        )
                          ] ;
              rdfs:range :Parameter ;
              rdfs:comment """Indicates the time cycle of how often the treatment, the follow-up test or their respective strategies have to be applied. In the case of a manifestation of the disease, it will indicate how many times a year the symptom manifests itself. 

By default = 1, for the Manifestations."""@en .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasGuideline
:hasGuideline rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Drug
                                          :FollowUpStrategy
                                          :FollowUpTest
                                          :LineOfTherapy
                                          :Treatment
                                        )
                          ] ;
              rdfs:range :Guideline .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasIncreasedMortalityRate
:hasIncreasedMortalityRate rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :hasParameter ;
                           rdfs:domain :Manifestation ;
                           rdfs:range :Parameter ;
                           rdfs:comment "By default = 1" .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasInitialProportion
:hasInitialProportion rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasParameter ;
                      rdfs:domain :Population ,
                                  [ rdf:type owl:Class ;
                                    owl:unionOf ( :ChronicManifestation
                                                  :Stage
                                                )
                                  ] ;
                      rdfs:range :Parameter ;
                      rdfs:comment "The initial proportion of patients of a population that already present certain manifestation"@en .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasIntervention
:hasIntervention rdf:type owl:ObjectProperty ;
                 rdfs:domain :Disease ;
                 rdfs:range :Intervention .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasLifeExpectancy
:hasLifeExpectancy rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasParameter ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :Development
                                               :Disease
                                               :Population
                                             )
                               ] ;
                   rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasLifeExpectancyReduction
:hasLifeExpectancyReduction rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :hasParameter ;
                            rdfs:domain :Manifestation ;
                            rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasLineOfTherapy
:hasLineOfTherapy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasStrategy ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Development
                                              :Disease
                                              :LineOfTherapy
                                              :Manifestation
                                              :TherapeuticIntervention
                                            )
                              ] ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( :LineOfTherapy
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :hasComponent ;
                                               owl:allValuesFrom :LineOfTherapy
                                             ]
                                           )
                             ] .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasManifestation
:hasManifestation rdf:type owl:ObjectProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Development
                                              :Disease
                                              :Stage
                                            )
                              ] ;
                  rdfs:range :Manifestation .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasNaturalDevelopment
:hasNaturalDevelopment rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasDevelopment .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasOnsetAge
:hasOnsetAge rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasParameter ;
             rdfs:domain :Manifestation ;
             rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasParameter
:hasParameter rdf:type owl:ObjectProperty ;
              owl:inverseOf :isParameterOf .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasProbabilityOfDeath
:hasProbabilityOfDeath rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasParameter ;
                       rdfs:domain :Manifestation ;
                       rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasProbabilityOfDiagnosis
:hasProbabilityOfDiagnosis rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :hasParameter ;
                           rdfs:domain :Manifestation ;
                           rdfs:range :Parameter ;
                           rdfs:comment "Probability that the manifestation presented, the clinical diagnosis is made." .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasProportionWithinGroup
:hasProportionWithinGroup rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :hasParameter ;
                          rdfs:domain :GroupableModelItem ;
                          rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasRiskCharacterization
:hasRiskCharacterization rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :hasParameter ;
                         rdfs:domain :DiseaseProgression ;
                         rdfs:range [ rdf:type owl:Class ;
                                      owl:unionOf ( :Parameter
                                                    :Pathway
                                                  )
                                    ] ;
                         :TODO "Check if this is the best name for the property" .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasScreeningStrategy
:hasScreeningStrategy rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasStrategy ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( :Disease
                                                  :ScreeningIntervention
                                                  :ScreeningStrategy
                                                )
                                  ] ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( :ScreeningStrategy
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasComponent ;
                                                   owl:allValuesFrom :ScreeningStrategy
                                                 ]
                                               )
                                 ] .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasSensitivity
:hasSensitivity rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasParameter ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :DetectionIntervention
                                            :DetectionStrategy
                                          )
                            ] ;
                rdfs:range :Parameter ;
                rdfs:comment "The Sensitivity in epidemiology is the Probability of correctly classifying a sick individual, that is, the Probability that a positive result will be obtained in a diagnostic test for a sick subject. The Sensitivity is, therefore, the ability of the complementary test to detect the disease. Sensitivity is the percentage of true positives or the Probability that the test is positive if the disease is present; False negatives are sick subjects diagnosed as healthy." .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasSex
:hasSex rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :hasAttributeValue ;
        rdfs:domain :Population ;
        rdfs:range [ rdf:type owl:Restriction ;
                     owl:onProperty :isValueOfAttribute ;
                     owl:hasValue :Attribute_Sex
                   ] .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasSpecificity
:hasSpecificity rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasParameter ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :DetectionIntervention
                                            :DetectionStrategy
                                          )
                            ] ;
                rdfs:range :Parameter ;
                rdfs:comment "Start Age" ,
                             "The Specificity of a test is the Probability that a healthy subject will have a negative test result. Specificity is the percentage of true negatives or the Probability that the test will be negative if the disease is not present. False positives are healthy subjects diagnosed as sick." .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasStage
:hasStage rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Development
                                      :Disease
                                    )
                      ] ;
          rdfs:range :Stage .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasStrategy
:hasStrategy rdf:type owl:ObjectProperty .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasSubpopulation
:hasSubpopulation rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isSubpopulationOf ;
                  rdfs:domain :Population ;
                  rdfs:range :Population .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasTemporaryThreshold
:hasTemporaryThreshold rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasParameter ;
                       rdfs:comment "Time limit in which the sample must be made. For example on the third day of birth. This value must be expressed in number of days." .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasTreatmentCost
:hasTreatmentCost rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasCost ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Development
                                              :Disease
                                              :Manifestation
                                            )
                              ] ;
                  rdfs:range :Cost .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasUncertaintyCharacterization
:hasUncertaintyCharacterization rdf:type owl:ObjectProperty ;
                                rdfs:domain [ rdf:type owl:Class ;
                                              owl:unionOf ( :FirstOrderUncertaintyParameter
                                                            :SecondOrderUncertaintyParameter
                                                          )
                                            ] ;
                                rdfs:range [ rdf:type owl:Class ;
                                             owl:unionOf ( :Parameter
                                                           :ProbabilityDistributionExpression
                                                         )
                                           ] .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasUtility
:hasUtility rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasParameter ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Development
                                        :Disease
                                        :Guideline
                                        :Intervention
                                        :Manifestation
                                        :Population
                                        :Stage
                                      )
                        ] ;
            rdfs:range :Utility .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#hasValue
:hasValue rdf:type owl:ObjectProperty ;
          owl:inverseOf :isValueOfAttribute ;
          rdfs:domain :Attribute ;
          rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#includedByModel
:includedByModel rdf:type owl:ObjectProperty ;
                 owl:inverseOf :includesModelItem ;
                 rdfs:domain :ModelItem ;
                 rdfs:range :Model .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#includesModelItem
:includesModelItem rdf:type owl:ObjectProperty ;
                   rdfs:domain :Model ;
                   rdfs:range :ModelItem .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#involvesModification
:involvesModification rdf:type owl:ObjectProperty ;
                      rdfs:domain :Intervention ;
                      rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#isModifiedBy
:isModifiedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :modifies ;
              rdfs:domain :Parameter ;
              rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#isParameterOf
:isParameterOf rdf:type owl:ObjectProperty ;
               rdfs:domain :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#isParameterOfDisease
:isParameterOfDisease rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :isParameterOf ;
                      rdfs:range :Disease .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#isParameterOfManifestation
:isParameterOfManifestation rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :isParameterOf ;
                            rdfs:range :Manifestation .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#isParameterOfPopulation
:isParameterOfPopulation rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :isParameterOf ;
                         rdfs:range :Population .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#isSubpopulationOf
:isSubpopulationOf rdf:type owl:ObjectProperty ;
                   rdfs:domain :Population ;
                   rdfs:range :Population .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#isValueOfAttribute
:isValueOfAttribute rdf:type owl:ObjectProperty ;
                    rdfs:domain :Parameter ;
                    rdfs:range :Attribute .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#modifies
:modifies rdf:type owl:ObjectProperty ;
          rdfs:domain :Parameter ;
          rdfs:range :Parameter .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#requires
:requires rdf:type owl:ObjectProperty ;
          rdfs:domain :DiseaseProgressionPathway ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Development
                                     :Intervention
                                     :Manifestation
                                     :Stage
                                   )
                     ] .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#usesDrug
:usesDrug rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :usesHealthTechnology ;
          rdfs:domain :LineOfTherapy ;
          rdfs:range :Drug .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#usesFollowUpTest
:usesFollowUpTest rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :usesHealthTechnology ;
                  rdfs:domain :FollowUpStrategy ;
                  rdfs:range :FollowUpTest .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#usesHealthTechnology
:usesHealthTechnology rdf:type owl:ObjectProperty .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#usesSameModelItemsAs
:usesSameModelItemsAs rdf:type owl:ObjectProperty ;
                      rdfs:domain :Model ;
                      rdfs:range :Model ;
                      rdfs:comment "Defines that a model uses the same data items as another model. Useful for avoiding \"recreating\" that relationship for all the model items in models which are very similar and simply difer in some characteristics." .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#usesTreatment
:usesTreatment rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :usesHealthTechnology ;
               rdfs:domain :LineOfTherapy ;
               rdfs:range :Treatment .


#################################################################
#    Data properties
#################################################################

###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#REMOVEhasFrequency
:REMOVEhasFrequency rdf:type owl:DatatypeProperty ;
                    rdfs:comment """Indicates the time cycle of how often the treatment, the follow-up test or their respective strategies have to be applied. In the case of a manifestation of the disease, it will indicate how many times a year the symptom manifests itself. 

By default = 1, for the Manifestations.""" .


###  http://www.ull.es/iis/simulation/ontologies/disease-simulation#appliesOneTime
:appliesOneTime rdf:type owl:DatatypeProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Cost
                                            :Utility
                                          )
                            ] ;
                rdfs:range xsd:boolean ;
